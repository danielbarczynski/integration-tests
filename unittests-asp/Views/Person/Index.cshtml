@model IEnumerable<PersonModel>

@{

}

<a asp-action="Create">
    <button>

        Create Person
    </button>
</a>

<main>
    <table class="table text-light">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Age)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Age
                    </td>
                </tr>
            }
        </tbody>
    </table>
</main>